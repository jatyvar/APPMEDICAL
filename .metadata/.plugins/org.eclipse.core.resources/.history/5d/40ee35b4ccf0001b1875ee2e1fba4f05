package interfaceGUI;

import javax.swing.*;

public class interfazGUI {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		JFrame ventana = new JFrame ("Mi Primer Ventana");
		ventana.setLayout(null);
		JLabel titulo = new JLabel("Hola Mundo");
		titulo.setBounds(150,20,100,20);
		ventana.add(titulo);
		JLabel texto = new JLabel("Esto es una prueba");
		texto.setBounds(130,60,100,20);
		ventana.add(texto);
		
		
		
		ventana.setVisible(true);
		ventana.setSize(400, 500);
		ventana.setDefaultCloseOperation(JFrame.EXIT_ON_CLOSE);
	}

}
