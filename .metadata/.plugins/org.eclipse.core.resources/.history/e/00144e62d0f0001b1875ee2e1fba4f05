package interfaceGUI;

import javax.swing.*;

public class interfazGUI {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		
		JFrame ventana = new JFrame ("Mi Primer Ventana");
		ventana.setLayout(null);
		
		JLabel titulo = new JLabel("Hola Mundo");
		titulo.setBounds(150,20,100,20);
		ventana.add(titulo);
		JLabel texto = new JLabel("DNI");
		texto.setBounds(80,100,150,20);
		ventana.add(texto);
		
		JTextField caja1 = new JTextField("Mision TIC 2022");
		caja1.setBounds(110,100,150,20);
		ventana.add(caja1);
		
		JButton boton1= new JButton("Aceptar");
		boton1.setBounds(130,150,100,20);
		ventana.add(boton1);
		
		ventana.setVisible(true);
		ventana.setSize(400, 500);
		ventana.setDefaultCloseOperation(JFrame.EXIT_ON_CLOSE);
	}

}
