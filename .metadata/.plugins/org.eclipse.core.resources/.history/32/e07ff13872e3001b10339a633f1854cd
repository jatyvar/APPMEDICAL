
public class principal {

	public static void main(String[] args) {
		Libro listaLibros[]= new Libro[100];
		// TODO Auto-generated method stub
		Libro libro1 = new Libro ("Algebra", "Baldor", 20, 0);
		Libro libro2 = new Libro ("Cien Años De Soledad", "Garcia Marquez", 30, 6);
		Libro libro3 = new Libro ("piedra filosofal", "K.Rowling", 10, 3);		
		Libreria Colombia = new Libreria("Libreria colombia", "Calle 16 # 10-56", 321345667, listaLibros);
		
		Colombia.agregarLibros(libro1, 0);
		Colombia.agregarLibros(libro2, 1);
		Colombia.agregarLibros(libro3, 2);
		Colombia.mostrarLibro(1)
		System.out.print(Colombia.mostrarLibro(1).toString());
	}

}
